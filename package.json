{
  "name": "supervision",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt -p 3005",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "build_pkg_linux": "mkdir -p pkg && rm -rf pkg/*.node && pkg --target=node16-linux-x64 -o pkg/docker-monitor scripts/docker/docker-monitor.js && cp ./node_modules/uWebSockets.js/uws_linux_*93.node pkg/",
    "build_app": "./scripts/build_app.sh",
    "build_all": "yarn build_pkg_linux && yarn build_app && chmod +x pkg/docker-monitor && chmod +x app/dist/supervision/supervision-*"
  },
  "main": "index.html",
  "engines": {
    "node": "v16"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dayjs": "^1.4.1",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.21.0",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.3",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.3",
    "cors": "^2.8.5",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.4.1",
    "nanoexpress": "^5.1.2",
    "node-ssh": "^12.0.3",
    "pkg": "^5.5.2",
    "prettier": "^2.5.1"
  },
  "packageManager": "yarn@3.1.1"
}
