{
  "name": "supervision",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt -p 3005",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "lintfix": "yarn lint:js --fix",
    "build_docker_monitor": "rm -rf pkg/docker-monitor && NODE_ENV=production pkg --target=node16-linux-x64 -o pkg/docker-monitor scripts/docker/docker-monitor.js",
    "build_uptime_monitor": "rm -rf pkg/uptime && NODE_ENV=production pkg --target=node16-linux-x64 -o pkg/uptime-monitor scripts/uptime/uptime-robot.js"
  },
  "main": "index.html",
  "engines": {
    "node": "v16"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dayjs": "^1.4.1",
    "@wtfcode/byte-converter": "^2.0.8",
    "cookie-universal-nuxt": "^2.2.2",
    "core-js": "^3.30.2",
    "node-ssh": "^13.1.0",
    "npm-check-updates": "^16.10.12",
    "nuxt": "^2.15.8",
    "p5": "^1.4.1",
    "tiny-json-http": "^7.5.1",
    "vue": "2.7.14",
    "vue-server-renderer": "^2.7.14",
    "vue-template-compiler": "^2.7.14",
    "vuetify": "^2.6.15",
    "webpack": "4.46.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@nuxt/types": "~2.15.0",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "eslint": "^8.19.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-vue": "^9.2.0",
    "pkg": "^5.8.1"
  },
  "packageManager": "yarn@3.2.1"
}
